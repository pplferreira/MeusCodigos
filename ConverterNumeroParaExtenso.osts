{"version":"0.2.0","body":"function main(workbook: ExcelScript.Workbook, NumeroExtenso: number) {\n\n    let t1 = [\"um\", \"dois\", \"três\", \"quatro\", \"cinco\", \"seis\", \"sete\", \"oito\", \"nove\"];\n    let t2 = [\"dez\", \"onze\", \"doze\", \"treze\", \"quatorze\", \"quinze\", \"dezesseis\", \"dezessete\", \"dezoito\", \"dezenove\"];\n    let t3 = [\"vinte\", \"trinta\", \"quarenta\", \"cinquenta\", \"sessenta\", \"setenta\", \"oitenta\", \"noventa\"];\n    let t4 = [\"cento\", \"duzentos\", \"trezentos\", \"quatrocentos\", \"quinhentos\", \"seiscentos\", \"setecentos\", \"oitocentos\", \"novecentos\"];\n    let n: number = NumeroExtenso;\n    let j: string;\n    let r: string;\n    let t: number;\n    let x: string;\n    let rTri: string;\n    let rBi: string;\n    let rMi: string;\n    let rMil: string;\n    let rZero: string;\n    let ri: string;\n    let rd: string;\n    let z: number;\n    let i: number;\n    let unidadesTri: number;\n    let dezenasTri: number;\n    let centenasTri: number;\n    let unidadesBi: number;\n    let dezenasBi: number;\n    let centenasBi: number;\n    let unidadesMi: number;\n    let dezenasMi: number;\n    let centenasMi: number;\n    let unidadesMil: number;\n    let dezenasMil: number;\n    let centenasMil: number;\n    let unidades: number;\n    let dezenas: number;\n    let centenas: number;\n    let casas: number;\n    let casastri: number;\n    let casasbi: number;\n    let casasmi: number;\n    let casasmil: number;\n    let casaszero: number;\n    let v1: number, v2: number, v3: number, v4: number, v5: number, v6: number;\n    let v4x: number, v4y: number;\n    let v3x: number, v3y: number;\n    let v2x: number, v2y: number;\n    let v1x: number, v1y: number;\n    rTri = \"\";\n    rBi = \"\";\n    rMi = \"\";\n    rMil = \"\";\n    rZero = \"\";\n    r = \"\";\n    rd = \"\";\n    ri = \"\";\n    j = \"\";\n    i = n;\n    z = n - i;\n    z = parseInt(z.toFixed(2));\n    z = z * 100;\n    z = parseInt(z.toFixed(0));\n    casas = i.toString().length;\n\n    if (n < 0) { return \"Erro: número negativo\"; }\n\n    if (z == 100) {\n        z = 0;\n        i = i + 1;\n    }\n\n    if (casas > 12) {\n        v5 = parseInt(i / 1000000000000);\n        casastri = v5.toString().length;\n        if (v5 > 0) {\n            if (v5 > 0) { unidadesTri = parseInt(v5.toString().substr(casastri - 1, 1)); }\n            if (v5 > 9) { dezenasTri = parseInt(v5.toString().substr(casastri - 2, 1)); }\n            if (v5 > 99) { centenasTri = parseInt(v5.toString().substr(casastri - 3, 1)); }\n            if (v5 == 100) { return \"cem\"; }\n            else {\n                if (centenasTri > 0) {\n                    rTri = rTri + t4[centenasTri - 1];\n                    if (dezenasTri > 0 || unidadesTri > 0) { rTri = rTri + \" e \"; }\n                }\n                if (dezenasTri > 1) {\n                    rTri = rTri + t3[dezenasTri - 2];\n                    if (unidadesTri > 0) { rTri = rTri + \" e \"; }\n                } else if (dezenasTri == 1 && unidadesTri >= 0) {\n                    rTri = rTri + t2[dezenasTri + unidadesTri - 1];\n                }\n                if (unidadesTri > 0 && dezenasTri != 1) {\n                    rTri = rTri + t1[unidadesTri - 1];\n                }\n            }\n            x = rTri;\n\n            if (v5 > 1) { ri = x + \" trilhões\"; } else { ri = x + \" trilhão\"; }\n        }\n    }\n\n    if (casas > 9) {\n        v4x = parseInt(i / 1000000000) * 1000000000;\n        v4y = parseInt(i / 1000000000000) * 1000000000000;\n        v4 = (v4x - v4y) / 1000000000;\n        casasbi = v4.toString().length;\n        if (v4 > 0) {\n            if (v5) { j = \", \"; } else { j = \" \"; }\n            if (v4 > 0) { unidadesBi = parseInt(v4.toString().substr(casasbi - 1, 1)); }\n            if (v4 > 9) { dezenasBi = parseInt(v4.toString().substr(casasbi - 2, 1)); }\n            if (v4 > 99) { centenasBi = parseInt(v4.toString().substr(casasbi - 3, 1)); }\n            if (v4 == 100) { return \"cem\"; }\n            else {\n                if (centenasBi > 0) {\n                    rBi = rBi + t4[centenasBi - 1];\n                    if (dezenasBi > 0 || unidadesBi > 0) { rBi = rBi + \" e \"; }\n                }\n                if (dezenasBi > 1) {\n                    rBi = rBi + t3[dezenasBi - 2];\n                    if (unidadesBi > 0) { rBi = rBi + \" e \"; }\n                } else if (dezenasBi == 1 && unidadesBi >= 0) {\n                    rBi = rBi + t2[dezenasBi + unidadesBi - 1];\n                }\n                if (unidadesBi > 0 && dezenasBi != 1) {\n                    rBi = rBi + t1[unidadesBi - 1];\n                }\n            }\n            x = rBi;\n\n            if (v4 > 1) { ri = ri + j + x + \" bilhões\"; } else { ri = ri + j + x + \" bilhão\"; }\n        }\n    }\n\n    if (casas > 6) {\n        v3x = parseInt(i / 1000000) * 1000000;\n        v3y = parseInt(i / 1000000000) * 1000000000;\n        v3 = (v3x - v3y) / 1000000;\n        casasmi = v3.toString().length;\n        if (v3 > 0) {\n            if (v4 + v5) { j = \", \"; } else { j = \" \"; }\n            if (v3 > 0) { unidadesMi = parseInt(v3.toString().substr(casasmi - 3, 1)); }\n            if (v3 > 9) { dezenasMi = parseInt(v3.toString().substr(casasmi - 2, 1)); }\n            if (v3 > 99) { centenasMi = parseInt(v3.toString().substr(casasmi - 1, 1)); }\n            if (v3 == 100) { return \"cem\"; }\n            else {\n                if (centenasMi > 0) {\n                    rMi = rMi + t4[centenasMi - 1];\n                    if (dezenasMi > 0 || unidadesMi > 0) { rMi = rMi + \" e \"; }\n                }\n                if (dezenasMi > 1) {\n                    rMi = rMi + t3[dezenasMi - 2];\n                    if (unidadesMi > 0) { rMi = rMi + \" e \"; }\n                } else if (dezenasMi == 1 && unidadesMi >= 0) {\n                    rMi = rMi + t2[dezenasMi + unidadesMi - 1];\n                }\n                if (unidadesMi > 0 && dezenasMi != 1) {\n                    rMi = rMi + t1[unidadesMi - 1];\n                }\n            }\n            x = rMi;\n\n            if (v3 > 1) { ri = ri + j + x + \" milhões\"; } else { ri = ri + j + x + \" milhão\"; }\n        }\n    }\n\n    if (casas > 3) {\n\n        v2x = parseInt(i / 1000) * 1000;\n        v2y = parseInt(i / 1000000) * 1000000;\n        v2 = (v2x - v2y) / 1000;\n        casasmil = v2.toString().length;\n        if (v2 > 0) {\n            if (v3 + v4 + v5) { j = \", \"; } else { j = \" \"; }\n            if (v2 > 0) { unidadesMil = parseInt(v2.toString().substr(casasmil - 1, 1)); }\n            if (v2 > 9) { dezenasMil = parseInt(v2.toString().substr(casasmil - 2, 1)); }\n            if (v2 > 99) { centenasMil = parseInt(v2.toString().substr(casasmil - 3, 1)); }\n            if (v2 == 100) { return \"cem\"; }\n            else {\n                if (centenasMil > 0) {\n                    rMil = rMil + t4[centenasMil - 1];\n                    if (dezenasMil > 0 || unidadesMil > 0) { rMil = rMil + \" e \"; }\n                }\n                if (dezenasMil > 1) {\n                    rMil = rMil + t3[dezenasMil - 2];\n                    if (unidadesMil > 0) { rMil = rMil + \" e \"; }\n                } else if (dezenasMil == 1 && unidadesMil >= 0) {\n                    rMil = rMil + t2[dezenasMil + unidadesMil - 1];\n                }\n                if (unidadesMil > 0 && dezenasMil != 1) {\n                    rMil = rMil + t1[unidadesMil - 1];\n                }\n            }\n            x = rMil;\n\n            ri = ri + j + x + \" mil \";\n\n        }\n    }\n\n    if (casas > 0) {\n\n        v1x = i;\n        v1y = Math.trunc(i / 1000) * 1000;\n        v1 = Math.abs(v1y - v1x);\n        casaszero = v1.toString().length;\n        if (v1 > 0) {\n            if (v2 + v3 + v4 + v5) { if (v1 <= 100) { j = \" e \"; } else { j = \", \"; } } else { j = \"\"; }\n            if (v1 > 0) { unidades = parseInt(v1.toString().substr(casaszero - 1, 1)); }\n            if (v1 > 9) { dezenas = parseInt(v1.toString().substr(casaszero - 2, 1)); }\n            if (v1 > 99) { centenas = parseInt(v1.toString().substr(casaszero - 3, 1)); }\n            if (v1 == 100) { return \"cem\"; }\n            else {\n                if (centenas > 0) {\n                    rZero = rZero + t4[centenas - 1];\n                    if (dezenas > 0 || unidades > 0) { rZero = rZero + \" e \"; }\n                }\n                if (dezenas > 1) {\n                    rZero = rZero + t3[dezenas - 2];\n                    if (unidades > 0) { rZero = rZero + \" e \"; }\n                } else if (dezenas == 1 && unidades >= 0) {\n                    rZero = rZero + t2[dezenas + unidades - 1];\n                }\n                if (unidades > 0 && dezenas != 1) {\n                    rZero = rZero + t1[unidades - 1];\n                }\n            }\n            x = rZero;\n\n            ri = ri + j + x;\n        }\n    }\n\n    return ri.trim();\n}","description":"","parameterInfo":"{\"originalParameterOrder\":[{\"name\":\"NumeroExtenso\",\"index\":0}],\"parameterSchema\":{\"type\":\"object\",\"required\":[\"NumeroExtenso\"],\"properties\":{\"NumeroExtenso\":{\"type\":\"number\"}}},\"returnSchema\":{\"type\":\"object\",\"properties\":{\"result\":{\"type\":\"string\"}}}}","apiInfo":"{\"variant\":\"synchronous\",\"variantVersion\":2}"}
